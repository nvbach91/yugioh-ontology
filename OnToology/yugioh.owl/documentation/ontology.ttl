#################################################################
#    Object Properties
#################################################################

###  http://ontology.ethereal.cz/ygo#hasEdition
:hasEdition rdf:type owl:ObjectProperty ;
            rdfs:domain :Card ;
            rdfs:range :Card_Edition ;
            rdfs:label "has edition"@en .


###  http://ontology.ethereal.cz/ygo#hasMonsterAbility
:hasMonsterAbility rdf:type owl:ObjectProperty ;
                   rdfs:domain :Monster_Card ;
                   rdfs:range :Monster_Ability ;
                   rdfs:label "has monster ability"@en .


###  http://ontology.ethereal.cz/ygo#hasMonsterAttribute
:hasMonsterAttribute rdf:type owl:ObjectProperty ;
                     rdfs:domain :Monster_Card ;
                     rdfs:range :Monster_Attribute ;
                     rdfs:label "has monster attribute"@en .


###  http://ontology.ethereal.cz/ygo#hasMonsterCategory
:hasMonsterCategory rdf:type owl:ObjectProperty ;
                    rdfs:domain :Monster_Card ;
                    rdfs:range :Monster_Category ;
                    rdfs:label "has monster category"@en .


###  http://ontology.ethereal.cz/ygo#hasMonsterType
:hasMonsterType rdf:type owl:ObjectProperty ;
                rdfs:domain :Monster_Card ;
                rdfs:range :Monster_Type ;
                rdfs:label "has monster type"@en .


###  http://ontology.ethereal.cz/ygo#hasRarity
:hasRarity rdf:type owl:ObjectProperty ;
           rdfs:domain :Card ;
           rdfs:range :Card_Rarity ;
           rdfs:label "has rarity"@en .


###  http://ontology.ethereal.cz/ygo#hasTournamentFormat
:hasTournamentFormat rdf:type owl:ObjectProperty ;
                     rdfs:domain :Tournament ;
                     rdfs:range :Tournament_Format ;
                     rdfs:label "has tournament format"@en .


###  http://ontology.ethereal.cz/ygo#hasTournamentLevel
:hasTournamentLevel rdf:type owl:ObjectProperty ;
                    rdfs:domain :Tournament ;
                    rdfs:range :Tournament_Level ;
                    rdfs:label "has tournament level"@en .


###  http://ontology.ethereal.cz/ygo#isPartOfMatch
:isPartOfMatch rdf:type owl:ObjectProperty ;
               rdfs:domain :Duel ;
               rdfs:range :Match ;
               rdfs:label "is part of match"@en .


###  http://ontology.ethereal.cz/ygo#isPartOfTournament
:isPartOfTournament rdf:type owl:ObjectProperty ;
                    rdfs:domain :Match ;
                    rdfs:range :Tournament ;
                    rdfs:label "is part of tournament"@en .


###  http://ontology.ethereal.cz/ygo#isUsedInDeck
:isUsedInDeck rdf:type owl:ObjectProperty ;
              rdfs:domain :Card ;
              rdfs:range :Deck ;
              rdfs:label "is used in deck"@en .


###  http://ontology.ethereal.cz/ygo#participatedInTournament
:participatedInTournament rdf:type owl:ObjectProperty ;
                          rdfs:domain :Duelist ;
                          rdfs:range :Tournament ;
                          rdfs:label "participated in tournament"@en .


###  http://ontology.ethereal.cz/ygo#playedAgainst
:playedAgainst rdf:type owl:ObjectProperty ;
               rdfs:domain :Duelist ;
               rdfs:range :Duelist ;
               rdfs:label "played against"@en .


###  http://ontology.ethereal.cz/ygo#playedInDuel
:playedInDuel rdf:type owl:ObjectProperty ;
              rdfs:domain :Duelist ;
              rdfs:range :Duel ;
              rdfs:label "played in duel"@en .


###  http://ontology.ethereal.cz/ygo#playedInMatch
:playedInMatch rdf:type owl:ObjectProperty ;
               rdfs:domain :Duelist ;
               rdfs:range :Match ;
               rdfs:label "played in match"@en .


###  http://ontology.ethereal.cz/ygo#usesDeck
:usesDeck rdf:type owl:ObjectProperty ;
          rdfs:domain :Duelist ;
          rdfs:range :Deck ;
          rdfs:label "uses deck"@en .


###  http://ontology.ethereal.cz/ygo/tookPlaceAt
<http://ontology.ethereal.cz/ygo/tookPlaceAt> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://dublincore.org/2012/06/14/dctype#Event> ;
                                              rdfs:range :Venue ;
                                              rdfs:comment "the place where the tournament or game is held"@en ;
                                              rdfs:label "took place at"@en .


#################################################################
#    Data properties
#################################################################

###  http://ontology.ethereal.cz/ygo#ATK
:ATK rdf:type owl:DatatypeProperty ;
     rdfs:domain :Monster_Card ;
     rdfs:range xsd:integer ;
     rdfs:comment "The ATK (attack) points of a Monster."@en ;
     rdfs:label "ATK"@en .


###  http://ontology.ethereal.cz/ygo#DEF
:DEF rdf:type owl:DatatypeProperty ;
     rdfs:domain :Monster_Card ;
     rdfs:range xsd:integer ;
     rdfs:comment "The DEF (defense) points of a Monster."@en ;
     rdfs:label "DEF"@en .


###  http://ontology.ethereal.cz/ygo#cardArchetype
:cardArchetype rdf:type owl:DatatypeProperty ;
               rdfs:domain :Card ;
               rdfs:comment "The archetype of a card"@en ;
               rdfs:label "card archetype"@en .


###  http://ontology.ethereal.cz/ygo#cardCount
:cardCount rdf:type owl:DatatypeProperty ;
           rdfs:domain :Deck ;
           rdfs:range xsd:integer ;
           rdfs:comment "The total number of Cards in a Deck."@en ;
           rdfs:label "card count"@en .


###  http://ontology.ethereal.cz/ygo#cardDescription
:cardDescription rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Card ;
                 rdfs:comment "The Card Text of Card Description of a Card."@en ;
                 rdfs:label "card description"@en ,
                            "card text"@en .


###  http://ontology.ethereal.cz/ygo#cardImage
:cardImage rdf:type owl:DatatypeProperty ;
           rdfs:domain :Card ;
           rdfs:range xsd:anyURI ;
           rdfs:comment "An URL refering to a visualisation of the Card."@en ;
           rdfs:label "card image"@en .


###  http://ontology.ethereal.cz/ygo#cardLimit
:cardLimit rdf:type owl:DatatypeProperty ;
           rdfs:domain :Card ;
           rdfs:range xsd:integer ;
           rdfs:comment "The number of Cards allowed in a Deck"@en ;
           rdfs:label "card limit"@en .


###  http://ontology.ethereal.cz/ygo#cardName
:cardName rdf:type owl:DatatypeProperty ;
          rdfs:domain :Card ;
          rdfs:comment "The name of the Card"@en ;
          rdfs:label "card name"@en .


###  http://ontology.ethereal.cz/ygo#cardSeriesNumber
:cardSeriesNumber rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Card ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "The unique identificator number of a Card."@en ;
                  rdfs:label "card series number"@en .


###  http://ontology.ethereal.cz/ygo#cardSet
:cardSet rdf:type owl:DatatypeProperty ;
         rdfs:domain :Card ;
         rdfs:comment "The name of the card set which a Card comes from."@en ;
         rdfs:label "card set"@en .


###  http://ontology.ethereal.cz/ygo#cossyID
:cossyID rdf:type owl:DatatypeProperty ;
         rdfs:domain :Duelist ;
         rdfs:range xsd:integer ;
         rdfs:comment "The Cossy ID of a Duelist."@en ;
         rdfs:label "Cossy ID"@en .


###  http://ontology.ethereal.cz/ygo#deckArchetype
:deckArchetype rdf:type owl:DatatypeProperty ;
               rdfs:domain :Deck ;
               rdfs:comment "The archetype of a Deck"@en ;
               rdfs:label "deck archetype"@en .


###  http://ontology.ethereal.cz/ygo#deckRating
:deckRating rdf:type owl:DatatypeProperty ;
            rdfs:domain :Deck ;
            rdfs:range xsd:integer ;
            rdfs:comment "The rating of a Deck. Usually determined by its strengths."@en ;
            rdfs:label "deck rating"@en .


###  http://ontology.ethereal.cz/ygo#duelResult
:duelResult rdf:type owl:DatatypeProperty ;
            rdfs:domain :Duel ;
            rdfs:range xsd:string ;
            rdfs:comment "A duel result. Either DRAW or Regular Win-Lose"@en ;
            rdfs:label "duel result"@en .


###  http://ontology.ethereal.cz/ygo#duelistRanking
:duelistRanking rdf:type owl:DatatypeProperty ;
                rdfs:domain :Duelist ;
                rdfs:range xsd:integer ;
                rdfs:comment "The ranking of a Duelist"@en ;
                rdfs:label "duelist ranking"@en .


###  http://ontology.ethereal.cz/ygo#level
:level rdf:type owl:DatatypeProperty ;
       rdfs:domain :Monster_Card ;
       rdfs:range xsd:integer ;
       rdfs:comment "The Level of a Monster"@en ;
       rdfs:label "level"@en .


###  http://ontology.ethereal.cz/ygo#linkRating
:linkRating rdf:type owl:DatatypeProperty ;
            rdfs:domain :Monster_Card ;
            rdfs:range xsd:integer ;
            rdfs:comment "The Link ranking of a Link Monster"@en ;
            rdfs:label "link ranking"@en .


###  http://ontology.ethereal.cz/ygo#matchResult
:matchResult rdf:type owl:DatatypeProperty ;
             rdfs:domain :Match ;
             rdfs:range xsd:string ;
             rdfs:comment "A match result. Either DRAW or Regular Win-Lose."@en ;
             rdfs:label "match result"@en .


###  http://ontology.ethereal.cz/ygo#monsterRank
:monsterRank rdf:type owl:DatatypeProperty ;
             rdfs:domain :Monster_Card ;
             rdfs:range xsd:integer ;
             rdfs:comment "The rank of a monster"@en ;
             rdfs:label "monster rank"@en .


###  http://ontology.ethereal.cz/ygo/venue_address
<http://ontology.ethereal.cz/ygo/venue_address> rdf:type owl:DatatypeProperty ;
                                                rdfs:domain :Venue ;
                                                rdfs:range xsd:string ;
                                                rdfs:comment "The address or location of the venue"@en ;
                                                rdfs:label "venue address"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Place
<http://dbpedia.org/ontology/Place> rdf:type owl:Class .


###  http://dublincore.org/2012/06/14/dctype#Event
<http://dublincore.org/2012/06/14/dctype#Event> rdf:type owl:Class .


###  http://ontology.ethereal.cz/ygo#Card
:Card rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:comment "There are three main types of cards: Monster, Spell, and Trap."@en ;
      rdfs:label "Card"@en .


###  http://ontology.ethereal.cz/ygo#Card_Edition
:Card_Edition rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              rdfs:comment "The editions of cards. usually determined by the time they are printed."@en ;
              rdfs:label "Card Edition"@en .


###  http://ontology.ethereal.cz/ygo#Card_Rarity
:Card_Rarity rdf:type owl:Class ;
             rdfs:subClassOf owl:Thing ;
             rdfs:comment "This refers to a rarity of a Card. It is classified on to several rarities."@en ;
             rdfs:label "Card Rarity"@en ;
             rdfs:seeAlso "http://yugioh.wikia.com/wiki/Rarity"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Continuous_Spell_Card
:Continuous_Spell_Card rdf:type owl:Class ;
                       rdfs:subClassOf :Spell_Card ;
                       rdfs:comment "This is a Spell Speed 1 Spell Card that stays on the field once it is activated. Spell Ruler, previously known as Magic Ruler, was the first TCG Booster Pack that contained Continuous Spell Cards."@en ;
                       rdfs:label "Continuous Spell Card"@en ;
                       rdfs:seeAlso "http://yugioh.wikia.com/wiki/Continuous_Spell_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Continuous_Trap_Card
:Continuous_Trap_Card rdf:type owl:Class ;
                      rdfs:subClassOf :Trap_Card ;
                      rdfs:comment "Continuous Traps are Trap Cards that stay on the field after activation. The effect(s) of these cards will remain active until the owner cannot maintain the cost or condition(s) indicated on the card (if any) OR it is destroyed. One example of a Continuous Trap Card is \"Spellbinding Circle\", which effectively freezes an enemy monster on the field, so it can neither change battle position nor declare an attack."@en ;
                      rdfs:label "Continuous Trap Card"@en ;
                      rdfs:seeAlso "http://yugioh.wikia.com/wiki/Continuous_Trap_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Counter_Trap_Card
:Counter_Trap_Card rdf:type owl:Class ;
                   rdfs:subClassOf :Trap_Card ;
                   rdfs:comment "Counter Trap Cards are a Spell Speed 3. Only other Counter Trap Cards can be chained to a Counter Trap Card"@en ;
                   rdfs:label "Counter Trap Card"@en ;
                   rdfs:seeAlso "http://yugioh.wikia.com/wiki/Counter_Trap_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Deck
:Deck rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:comment "A pile of cards that a Duelist can draw from during play. Each Duelist uses his or her own Main Deck in a Duel."@en ;
      rdfs:label "Deck"@en ;
      rdfs:seeAlso "http://yugioh.wikia.com/wiki/Deck"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Duel
:Duel rdf:type owl:Class ;
      rdfs:subClassOf <http://dublincore.org/2012/06/14/dctype#Event> ;
      rdfs:comment "A Duel is a single game between 2 opposing players or teams, they Duel each other using a variety of Monster, Spell, and Trap Cards to defeat their opponent and win the game."@en ;
      rdfs:label "Duel"@en ;
      rdfs:seeAlso "http://yugioh.wikia.com/wiki/Duel"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Duelist
:Duelist rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
         rdfs:comment "A Duelist or a Player is anyone participating in a Duel."@en ;
         rdfs:label "Duelist"@en ;
         rdfs:seeAlso "http://yugioh.wikia.com/wiki/Duelist"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Equip_Spell_Card
:Equip_Spell_Card rdf:type owl:Class ;
                  rdfs:subClassOf :Spell_Card ;
                  rdfs:comment "This is a Spell Card that is a card that is equipped to a face-up monster on the field. Equip Spell Cards are type of Equip Card; Equip Cards can either be Equip Spell Cards or Trap Cards that are treated as Equip Cards."@en ;
                  rdfs:label "Equip Spell Card"@en ;
                  rdfs:seeAlso "http://yugioh.wikia.com/wiki/Equip_Spell_card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Field_Spell_Card
:Field_Spell_Card rdf:type owl:Class ;
                  rdfs:subClassOf :Spell_Card ;
                  rdfs:comment "Field Spells have the advantage of being able to change the entire state of play for its controller's or for both players' side of the field. They are Spell Speed 1 and are placed on the Field Zone, apart from the Spell & Trap Zones."@en ;
                  rdfs:label "Field Spell Card"@en ;
                  rdfs:seeAlso "http://yugioh.wikia.com/wiki/Field_Spell_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Match
:Match rdf:type owl:Class ;
       rdfs:subClassOf <http://dublincore.org/2012/06/14/dctype#Event> ;
       rdfs:comment """A Match is a best-of-three set of Duels.

The first Duelist to win two Duels is declared the winner of the Match. If a Duelist loses two Duels, they are considered to have lost the Match. If both Duelists have one loss in a Match and both receive a simultaneous Game Loss penalty, the Match is considered a Double Loss."""@en ;
       rdfs:label "Match"@en ;
       rdfs:seeAlso "http://yugioh.wikia.com/wiki/Match"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Monster_Ability
:Monster_Ability rdf:type owl:Class ;
                 rdfs:subClassOf owl:Thing ;
                 rdfs:comment "Classification of Effect Monster Cards. Abilities are listed in the same line as a monster's Type, separated by a slash (but are not the only classification that can be listed this way)"@en ;
                 rdfs:label "Monster Ability"@en ;
                 rdfs:seeAlso "http://yugioh.wikia.com/wiki/Ability"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Monster_Attribute
:Monster_Attribute rdf:type owl:Class ;
                   rdfs:subClassOf owl:Thing ;
                   rdfs:comment "Attributes are essentially the \"elemental\" groups for each Monster Card. There are different cards made to support each \"element\", and there are popular monsters of each kind. It is possible for a monster to be considered part of multiple Attributes via a card effect, such the effect of \"Elemental Mistress Doriado\"."@en ;
                   rdfs:label "Monster Attribute"@en ;
                   rdfs:seeAlso "http://yugioh.wikia.com/wiki/Attribute"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Monster_Card
:Monster_Card rdf:type owl:Class ;
              rdfs:subClassOf :Card ;
              rdfs:comment "Monster cards represent monsters that players battle against each other or directly against either player during the Battle Phase. Monsters are the main focus of Duel Monsters. Monster Cards are differentiated by their names, Type; Attribute; ATK; DEF; Level (or Rank or Link Rating). Monster Cards can be subdivided into Normal Monsters, Effect Monsters, Ritual Monsters, Fusion Monsters, Synchro Monsters, Xyz Monsters, Pendulum Monsters, Link Monsters and Token Monsters, each with a distinctive colored card frame to differentiate them."@en ;
              rdfs:label "Monster Card"@en ;
              rdfs:seeAlso "http://yugioh.wikia.com/wiki/Monster_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Monster_Category
:Monster_Category rdf:type owl:Class ;
                  rdfs:subClassOf owl:Thing ;
                  rdfs:comment "Monster Cards can be subdivided into Normal Monsters, Effect Monsters, Ritual Monsters, Fusion Monsters, Synchro Monsters, Xyz Monsters, Pendulum Monsters, Link Monsters and Token Monsters, each with a distinctive colored card frame to differentiate them."@en ;
                  rdfs:label "Monster Category"@en ;
                  rdfs:seeAlso "http://yugioh.wikia.com/wiki/Monster_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Monster_Type
:Monster_Type rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              rdfs:comment "This is a characteristic of a Monster Card. All monsters have one Type, such that monsters of the same Type can be grouped together (in a similar way to Attributes) and support cards can grant various effects to a specific Type of monster."@en ;
              rdfs:label "Monster Type"@en ;
              rdfs:seeAlso "http://yugioh.wikia.com/wiki/Type"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Normal_Spell_Card
:Normal_Spell_Card rdf:type owl:Class ;
                   rdfs:subClassOf :Spell_Card ;
                   rdfs:comment "This is a Spell Speed 1 Spell Card, so it cannot chain to other effects, even those of other Normal Spell Cards, but can be chained to by Spell Speed 2 or Spell Speed 3 cards."@en ;
                   rdfs:label "Normal Spell Card"@en ;
                   rdfs:seeAlso "http://yugioh.wikia.com/wiki/Normal_Spell_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Normal_Trap_Card
:Normal_Trap_Card rdf:type owl:Class ;
                  rdfs:subClassOf :Trap_Card ;
                  rdfs:comment "Normal Traps are Spell Speed 2. They can be activated during either player's turn as long as it wasn't set that turn. The effects of certain Normal Trap Cards behave like Equip Cards, but the card is still considered a Normal Trap Card."@en ;
                  rdfs:label "Normal Trap Card"@en ;
                  rdfs:seeAlso "http://yugioh.wikia.com/wiki/Normal_Trap_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Quick-Play_Spell_Card
:Quick-Play_Spell_Card rdf:type owl:Class ;
                       rdfs:subClassOf :Spell_Card ;
                       rdfs:comment "Quick-play Spells are a type of Spell Card that are Spell Speed 2. The turn player can activate Quick-Play Spell Cards from their hand during any Phase of their turn; either player can activate Set Quick-Play Spell cards during any Phase in either player's turn, except during the turn they are Set."@en ;
                       rdfs:label "Quick-Play Spell Card"@en ;
                       rdfs:seeAlso "http://yugioh.wikia.com/wiki/Quick-Play_Spell_Card"^^xsd:anyURI .


###  http://ontology.ethereal.cz/ygo#Ritual_Spell_Card
:Ritual_Spell_Card rdf:type owl:Class ;
                   rdfs:subClassOf :Spell_Card ;
                   rdfs:comment "Ritual Spells are a type of Spell Card used to Ritual Summon Ritual Monsters."@en ;
                   rdfs:label "Ritual Spell Card"@en ;
                   rdfs:seeAlso "http://yugioh.wikia.com/wiki/Ritual_Spell_Card"^^xsd:anyURI .


